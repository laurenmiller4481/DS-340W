,"  pr = nx.pagerank(P, alpha=0.85)\n","  cc = nx.clustering(P)\n","\n","  # creating the features data\n","  df['pagerank'] = [pr[i] for i in df.product_ID]\n","  df['eigenvector_cent'] = [eig_cent[i] for i in df.product_ID]\n","  df['clustering_coef'] = [cc[i] for i in df.product_ID]\n","  df['w_degree'] = [w_degree_cent[i] for i in df.product_ID]\n","  return df\n","\n","\n","X_train_network = obtain_network_features(X_train)\n","X_validation_network = obtain_network_features(X_validation)\n","X_test_network = obtain_network_features(X_test)\n","\n","X_train_with_network = X_train_network.merge(X_train, on='product_ID', how='inner')\n","X_validation_with_network = X_validation_network.merge(X_validation, on='product_ID', how='inner')\n","X_test_with_network = X_test_network.merge(X_test, on='product_ID', how='inner')\n","\n","print(X_train_with_network.shape)\n","print(X_validation_with_network.shape)\n","print(X_test_with_network.shape)"]},{"cell_type":"code","execution_count":null,"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"elapsed":30657,"status":"ok","timestamp":1761241500080,"user":{"displayName":"Lauren Miller","userId":"12471227718816498567"},"user_tz":240},"id":"FC-n6UjlIeJ1","outputId":"f4d3bdcb-f413-45d8-a36e-a7b82af95dc8"},"outputs":[{"output_type":"stream","name":"stdout","text":["240000\n","[['poor', 'qualiti', 'disappoint', 'qualiti', 'product', 'spode', 'made', 'england', 'howev', 'plate', 'surfac', 'uneven', 'small', 'factori', 'defect', 'normal', 'find', 'supermarket', 'plate', 'someth', 'expect', 'spode', 'especi', 'price', 'paid', 'also', 'plate', 'thick', 'fine', 'bone', 'china', 'son', 'crazi', 'bunni', 'reason', 'got'], ['nice', 'plate', 'beauti'], ['beauti', 'beauti'], ['five', 'star', 'beauti']]\n"]}],"source":["X_train_text_tokenized = []\n","\n","for i in X_train_with_network[\"text\"]:\n","    temp = []\n","    for j in word_tokenize(i):\n","        temp.append(j.lower())\n","\n","    X_train_text_tokeni